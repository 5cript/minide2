{
  "name": "electron-react-typescript-app",
  "authors": "5cript",
  "description": "A minimal IDE",
  "version": "0.1.0",
  "private": true,
  "main": "src/windows/main_window.js",
  "build": {
    "appId": "minide2",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
