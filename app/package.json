    "ace-builds": "^1.4.12",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "crypto-js": "^4.0.0",
    "electron": "^13.1.7",
    "electron-is-dev": "^2.0.0",
    "electron-localshortcut": "^3.2.1",
