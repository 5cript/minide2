{
  "name": "electron-react-typescript-app",
  "authors": "5cript",
  "description": "A minimal IDE",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "build": {
    "appId": "minide2",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "IDE",
      "iconUrl": "public/resources/images/toolbar/build_run.png"
    },
    "mac": {
      "category": "IDE",
      "iconUrl": "public/resources/images/toolbar/build_run.png"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@monaco-editor/react": "^3.3.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "ace-builds": "^1.4.11",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "electron-compile": "^6.4.4",
    "electron-is-dev": "^1.2.0",
    "electron-localshortcut": "^3.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "extends-classes": "^1.0.5",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.19.3",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "rc-tree": "^3.3.0",
    "react": "^16.13.1",
    "react-accessible-accordion": "^3.0.1",
    "react-ace": "^9.0.0",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.36.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.0",
    "react-resize-detector": "^4.2.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.0",
    "react-scrollbars-custom": "^4.0.25",
    "react-spinners": "^0.8.3",
    "react-splitter-layout": "^4.0.0",
    "react-tabs": "^3.1.1",
    "react-treebeard": "^3.2.4",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "rodal": "^1.8.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "react-start": "react-app-rewired start",
    "start": "electron-forge start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^6.0.3",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.4",
    "react-app-rewired": "^2.1.6",
    "redux-devtools": "^3.5.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "electron_react_typescript_app"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "electronreacttypescriptapp"
      }
    }
  }
}
